<% if current_user.present? %>
  <div class="flex flex-col items-center my-12">
    <%= image_tag current_user.image, class: "rounded-full h-20 w-20" %>
    <p><%= current_user.name %></p>
    <p><%= current_user.email %></p>
  </div>
<% end %>

<%= render 'feed/list', actions_by_date: @actions_by_date %>
<%= paginate(@actions_by_date) %>
